<div class="weatherWidget">
  {{#each partials.weatherData.locations}}
    <div class="location">
      <h3>{{name}}</h3>
      <a href="{{forecastUrl}}">
        <img src="{{iconUrl}}" alt="{{weather}}">
        {{weather}}, {{temp}}
      </a>
    </div>
    {{#else}}
      No weather info
    {{/else}}
  {{/each}}
  <small>Source: <a href="http://www.wunderground.com">Weather Underground</a></small>
</div>
